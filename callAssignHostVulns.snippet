$hostVulns$=none;
$potentialVulns$=$vulnsHost$;


if($potentialVulns$ != none){
  for($potVuln$ in $potentialVulns$){
    $curVuln$=[$potVuln$];
    $curVuln$+=["none"];
    if($potVuln$ == "meltdown"){
      $curProb$=$meltdownProb$;
    };

    $outcome$=$PROB($curVuln$,$curProb$)$;
    if($outcome$ != "none"){
      if($hostVulns$ != none){
        $hostVulns$+=[$outcome$];
      };
      if($hostVulns$ == none){
        $hostVulns$=[$outcome$];
      };
    };
  };
};
